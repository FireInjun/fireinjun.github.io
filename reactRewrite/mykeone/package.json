{
  "name": "mykeone",
  "version": "0.1.0",
  "description": "React based dev portfolio",
  "author": "FireInjun",
  "repository": {
    "url": "https://github.com/fireinjun/mkone.git",
    "website": "https://mykekenny.com"
  },
  "license": "MIT",
  "main": "index.js",
  "private": true,
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "pretty": "pretty-quick --write --tab-width 2 \"src/**/*.js\""
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "npm run pretty",
      "git add"
    ]
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "dependencies": {
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "react-router": "^4.3.1",
    "react-scripts": "2.1.3"
  },
  "devDependencies": {
    "husky": "^1.3.1",
    "lint-staged": "^8.1.0",
    "pretty-quick": "^1.10.0"
  }
}
